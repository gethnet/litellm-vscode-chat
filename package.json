{
	"name": "litellm-connector-copilot-chat",
	"publisher": "gethnet",
	"displayName": "LiteLLM Connector for GitHub Copilot Chat",
	"description": "An extension that integrates LiteLLM proxy into GitHub Copilot Chat",
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/gethnet/litellm-vscode-chat"
	},
	"version": "0.2.1-dev",
	"preview": true,
	"engines": {
		"vscode": "^1.107.0"
	},
	"categories": [
		"AI",
		"Chat",
		"Machine Learning",
		"Other"
	],
	"galleryBanner": {
		"color": "#100f11",
		"theme": "dark"
	},
	"badges": [
		{
			"url": "https://img.shields.io/github/stars/gethnet/litellm-vscode-chat?style=social",
			"description": "Star litellm-vscode-chat on Github",
			"href": "https://github.com/gethnet/litellm-vscode-chat"
		}
	],
	"homepage": "https://github.com/gethnet/litellm-vscode-chat",
	"bugs": {
		"url": "https://github.com/gethnet/litellm-vscode-chat/issues"
	},
	"license": "MIT",
	"activationEvents": [
		"onLanguageModelChatProvider:litellm-connector"
	],
	"contributes": {
		"languageModelChatProviders": [
			{
				"vendor": "litellm-connector",
				"displayName": "LiteLLM",
				"managementCommand": "litellm-connector.manage"
			}
		],
		"commands": [
			{
				"command": "litellm-connector.manage",
				"title": "Manage LiteLLM Provider"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"download-api": "dts dev",
		"postdownload-api": "dts main && node -e \"const fs = require('fs'); if (fs.existsSync('vscode.d.ts')) { fs.renameSync('vscode.d.ts', 'src/vscode.d.ts'); console.log('Moved vscode.d.ts to src/'); }\"",
		"postinstall": "npm run download-api",
		"compile": "tsc -p ./",
		"lint": "eslint --fix",
		"format": "prettier --write .",
		"watch": "tsc -watch -p ./",
		"test": "npm run compile && vscode-test",
		"bump-version": "node scripts/bump-version.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "^22",
		"@types/vscode": "^1.104.0",
		"@vscode/dts": "^0.4.1",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.13.0",
		"prettier": "^3.1.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0",
		"vsce": "^2.15.0"
	},
	"extensionDependencies": [
		"github.copilot-chat"
	],
	"packageManager": "pnpm@8.15.4+sha512.0bd3a9be9eb0e9a692676deec00a303ba218ba279d99241475616b398dbaeedd11146f92c2843458f557b1d127e09d4c171e105bdcd6b61002b39685a8016b9e"
}
